<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns="http://www.springframework.org/schema/beans"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- SAP CPI Outbound Service -->
	<alias name="sapCpiDefaultOutboundService" alias="sapCpiOutboundService" />
	<bean id="sapCpiDefaultOutboundService" class="de.hybris.platform.sap.sapcpiadapter.service.impl.SapCpiOutboundServiceImpl">
		<property name="outboundServiceFacade" ref="outboundServiceFacade" />
	</bean>

</beans>
